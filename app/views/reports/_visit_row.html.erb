<tr>
  <td class="px-6 py-4 whitespace-nowrap text-sm text-gray-500">
    <%= visit.ip_address || 'Unknown' %>
  </td>
  <td class="px-6 py-4 whitespace-nowrap text-sm text-gray-500">
    <%= visit.geolocation || 'Unknown' %>
  </td>
  <td class="px-6 py-4 whitespace-nowrap text-sm text-gray-500">
    <%= visit.timestamp.strftime("%Y-%m-%d %H:%M:%S") %>
  </td>
</tr>
